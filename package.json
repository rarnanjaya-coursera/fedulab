{
  "name": "fedulab",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.12",
    "@storybook/addon-links": "^3.2.12",
    "@storybook/react": "^3.2.12",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.55.0",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.0",
    "prettier-eslint": "^8.1.1",
    "prettier-eslint-cli": "^4.3.2",
    "react-scripts": "1.0.13",
    "react-toolbox-themr": "^1.0.2",
    "source-map-explorer": "^1.5.0"
  },
  "dependencies": {
    "@coursera/coursera-ui": "0.2.20-rc",
    "antd": "^2.13.4",
    "aphrodite": "^1.2.4",
    "auth0-lock": "^10.22.0",
    "classnames": "^2.2.5",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-15": "^1.0.0",
    "enzyme-to-json": "^3.0.0",
    "firebase-tools": "^3.13.1",
    "js-search": "^1.4.2",
    "material-design-icons": "^3.0.1",
    "moment": "^2.18.1",
    "optics-agent": "^1.1.7",
    "prop-types": "^15.6.0",
    "random-string": "^0.2.0",
    "rc-banner-anim": "^0.5.16",
    "rc-queue-anim": "^1.2.3",
    "rc-tween-one": "^1.4.5",
    "react": "^15.6.2",
    "react-apollo": "^1.4.16",
    "react-dom": "^15.6.2",
    "react-dropzone": "^4.2.1",
    "react-json-viewer": "^1.6.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-slick": "^0.15.4",
    "react-test-renderer": "^15.5.0",
    "react-toolbox": "2.0.0-beta.6",
    "recompose": "^0.25.1",
    "slick-carousel": "^1.8.1",
    "storybook": "^1.0.0",
    "string-hash": "^1.1.3",
    "styled-components": "^2.2.0",
    "surge": "^0.19.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "NODE_PATH=. react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "test:cover": "yarn run test -- --coverage",
    "flow": "node_modules/.bin/flow",
    "format": "prettier-eslint --write \"src/**/*.js\" ",
    "precommit": "lint-staged",
    "prettier": "prettier-eslint",
    "light": "lighthouse: http://localhost:5000",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "deploy": "npm run build && firebase deploy",
    "toolbox": "react-toolbox-themr"
  },
  "lint-staged": {
    "src/*.+(js|jsx)": [
      "prettier-eslint --parser flow --write",
      "git add"
    ]
  },
  "reactToolbox": {
    "include": [
      "BUTTON",
      "DATE_PICKER",
      "MENU",
      "AVATAR",
      "INPUT",
      "SWITCH"
    ],
    "customProperties": {
      "animation-duration": "0.3s",
      "color-primary": "var(--palette-deep-orange-700)",
      "color-accent": "var(--palette-light-blue-a200)",
      "color-accent-dark": "var(--palette-light-blue-700)",
      "color-primary-contrast": "var(--color-dark-contrast)",
      "color-accent-contrast": "var(--color-dark-contrast)"
    },
    "output": "assets/react-toolbox"
  }
}